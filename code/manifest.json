{
  "manifest_version": 2,
  "name": "BigBlueButton Smiley",
  "description": "Erm√∂glicht es Smilies in einem BigBlueButton-Chat oder Moodle zu verwenden",
  "version": "1.0",
  "author": "Lukas Kurz",
  "permissions": [
    "https://*/html5client/join?sessionToken=*",
    "https://*.lehrerfortbildung-bw.de/*",
    "https://*/moodle/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/html5client/join?sessionToken=*",
        "https://*.lehrerfortbildung-bw.de/*",
        "https://*/moodle/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "25": "res/icon25.png",
    "48": "res/icon48.png"
  }
}