{
  "manifest_version": 2,
  "name": "BigBlueButton Extensions",
  "description": "Erleichtert die Bediehnung von BBB",
  "version": "2.0",
  "author": "Lukas Kurz",
  "permissions": [
    "https://*/html5client/join?sessionToken=*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/html5client/join?sessionToken=*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "icons": {
    "25": "res/icon25.png",
    "48": "res/icon48.png"
  }
}